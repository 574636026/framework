package com.injedu.bookproduce.core.model;

import java.util.ArrayList;
import java.util.List;

/**
 * 
 * 题目章节
 *
 * @author joy.zhou
 * @date 2016年8月4日
 * @version 1.0
 *
 */
public class ProduceBookTopic extends ProduceBookArticle {

	private static final long serialVersionUID = 1L;
	/** 题型名称 */
	private String topicTypeName;
	/** 题干 */
	private String stem;
	/** 选项 */
	private String[] option;
	/** 正确的结果 */
	private String result;
	/** 解析 */
	private String analysis;
	/** 重点（点拨） */
	private String point;
	/** 子题目 */
	private List<ProduceBookTopic> childs = new ArrayList<>(0);

	public String getTopicTypeName() {
		return topicTypeName;
	}

	public void setTopicTypeName(String topicTypeName) {
		this.topicTypeName = topicTypeName;
	}

	public String getStem() {
		return stem;
	}

	public void setStem(String stem) {
		this.stem = stem;
	}

	public String[] getOption() {
		return option;
	}

	public void setOption(String[] option) {
		this.option = option;
	}

	public String getResult() {
		return result;
	}

	public void setResult(String result) {
		this.result = result;
	}

	public String getAnalysis() {
		return analysis;
	}

	public void setAnalysis(String analysis) {
		this.analysis = analysis;
	}

	public String getPoint() {
		return point;
	}

	public void setPoint(String point) {
		this.point = point;
	}

	public List<ProduceBookTopic> getChilds() {
		return childs;
	}

	public void setChilds(List<ProduceBookTopic> childs) {
		this.childs = childs;
	}

}
